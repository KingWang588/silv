<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Light"></style>

    <style name="AppTheme" parent="AppBaseTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="StartTheme" parent="Theme.AppCompat.Light.NoActionBar">
<!--        <item name="android:windowContentOverlay">@null</item>-->
<!--        <item name="android:windowBackground">@drawable/startup2</item>-->
    </style>

    <style name="SwitchTextAppearance" parent="@android:style/TextAppearance.Holo.Small">
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="grayline">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/ccgrey</item>
    </style>

    <style name="whiteline">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="greyvline">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/app_bg</item>
    </style>

    <style name="bulevline">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/textbule</item>
    </style>

    <style name="title_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
    </style>

    <style name="title_bule_layout">
        <item name="android:background">@color/textbule</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
    </style>
    <!-- popupwindow style -->
    <style name="AnimationPreview">
        <item name="android:windowEnterAnimation">@anim/sharepopup_s</item>
        <item name="android:windowExitAnimation">@anim/sharepopup_e</item>
    </style>

    <style name="RatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/ratingbar</item>
        <item name="android:minHeight">16dp</item>
        <item name="android:maxHeight">16dp</item>
    </style>

    <style name="Transparent">
        <item name="android:windowBackground">@color/transparent</item>  
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>  
         <item name="android:windowAnimationStyle">@null</item>  
    </style>

    <style name="SwitchTheme" parent="@android:style/TextAppearance.Small">
        <item name="android:textColor">@color/colorPrimary</item>
    </style>
</resources>